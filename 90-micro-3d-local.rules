# Micro 3D Printer rules file 
# Run the following to install this rule
# cp ./90-micro-3d-local.rules /etc/udev/rules.d/
# sudo /etc/init.d/udev restart

ACTION=="add", SUBSYSTEMS=="usb", ATTRS{idVendor}=="03eb", ATTRS{idProduct}=="2404", GROUP="users", MODE="0666", SYMLINK="micro_3d", ENV{ID_MM_DEVICE_IGNORE}="1"
